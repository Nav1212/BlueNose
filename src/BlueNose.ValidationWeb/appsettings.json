{
  "Fhir": {
    "Version": "R4",
    "ValidateOnParse": true,
    "StrictValidation": false,
    "TimeoutSeconds": 30
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "BlueNose": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Mcp": {
    "Enabled": true,
    "Endpoint": "/mcp"
  }
}
